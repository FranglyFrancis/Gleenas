                                    %>
                               
                                <tr>
                                    <td>
                                        <a class="itemside" href="#">
                                            <div class="left">
                                                <img src="productImages/<%= products[i].product.image[0] %>" width="40" height="40" class="img-xs" alt="Item">
                                            </div>
                                            <div class="info"> <%= products[i].product.name %> </div>
                                        </a>
                                    </td>
                                    <td> <%= products[i].product.price %></td>
                                    <td>  <%= products[i].quantity %> </td>
                                    <td class="text-end">  <%= subtotal[i].orders[0].subtotal %> </td>
                                </tr>
                               <%
                            }
                               %>
                                <tr>
